{
  "name": "willihaveasnowday",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "build": "NODE_ENV=production brunch build --production",
    "start": "concurrently -n \"BRUNCH,FLASK\" -c \"green.bold,cyan.bold\" \"npm:brunch-dev\" \"npm:flask-server\"",
    "brunch-dev": "NODE_ENV=debug brunch watch",
    "flask-server": "flask run",
    "run-production": "npm run build && gunicorn \"app.app:create_app()\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HHousen/willihaveasnowday.git"
  },
  "author": "Hayden Housen",
  "license": "Copyright Hayden Housen",
  "bugs": {
    "url": "https://github.com/HHousen/willihaveasnowday/issues"
  },
  "homepage": "https://github.com/HHousen/willihaveasnowday#readme",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "brunch": "^3.0.0"
  },
  "devDependencies": {
    "brunch": "^3.0.0",
    "concurrently": "^5.3.0",
    "postcss-brunch": "^3.0.0",
    "sass-brunch": "^3.0.0",
    "terser-brunch": "^4.0.0"
  }
}
